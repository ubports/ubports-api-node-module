  "homepage": "https://github.com/ubports/ubports-api-node-module#readme",
  "dependencies": {
    "request": "^2.83.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "nyc": "^14.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
